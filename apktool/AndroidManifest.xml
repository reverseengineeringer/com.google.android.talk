</service>
        <activity android:icon="@drawable/ic_launcher_google_talk" android:name="PublicIntentDispatcher" android:noHistory="true" android:theme="@android:style/Theme.Translucent.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.SENDTO"/>
