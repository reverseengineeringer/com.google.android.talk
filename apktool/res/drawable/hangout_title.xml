<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/hangout_title_background_pressed" />
            <corners android:radius="@dimen/hangout_title_background_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/hangout_title_background" />
            <corners android:radius="@dimen/hangout_title_background_radius" />
        </shape>
    </item>
</selector>