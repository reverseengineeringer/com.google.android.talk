<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <size android:height="@dimen/quick_button_size" android:width="@dimen/quick_button_size" />
            <solid android:color="@color/quick_button_pressed_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size android:height="@dimen/quick_button_size" android:width="@dimen/quick_button_size" />
            <solid android:color="@color/quick_button_unpressed_color" />
        </shape>
    </item>
</selector>